// NCI Configuration
//ALLOW_MISSING_DEPENDENCIES = ["true"]
android_app {
    name: "NQNfcNci",
    srcs: [
        "src/**/*.java",
        "nci/**/*.java",
    ],
    platform_apis: true,
    certificate: "platform",
    jni_libs: ["libsn100nfc_nci_jni"],
    libs: ["com.nxp.nfc.nq"],
    static_libs: [
        "androidx.legacy_legacy-support-v4",
        "vendor.nxp.hardware.nfc-V1.1-java",
        ],
    optimize: {
        enabled: false,
    },
}
